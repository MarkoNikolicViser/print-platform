{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "order_code": {
      "type": "string",
      "required": true
    },
    "status_code": {
      "type": "enumeration",
      "required": true,
      "default": "uploaded",
      "enum": [
        "draft",
        "uploaded",
        "paid",
        "printing",
        "ready",
        "picked_up",
        "cancelled",
        "expired"
      ]
    },
    "finish_code": {
      "type": "enumeration",
      "enum": [
        "success",
        "expired",
        "user_cancelled",
        "shop_cancelled",
        "partial",
        "error"
      ]
    },
    "total_price": {
      "type": "decimal",
      "required": true
    },
    "customer_email": {
      "type": "email"
    },
    "customer_phone": {
      "type": "string",
      "required": false
    },
    "print_shop_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::print-shop.print-shop",
      "inversedBy": "orders"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "order"
    },
    "expires_at": {
      "type": "datetime"
    },
    "estimated_completion_at": {
      "type": "datetime"
    },
    "completed_at": {
      "type": "datetime"
    },
    "completed_on_time": {
      "type": "boolean"
    }
  }
}